      "targets": [
        {
          "datasource": {
            "type": "mysql",
            "uid": "${DS_MYSQL_ZABBIX}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n    FROM_UNIXTIME(hl.clock) AS Horario,\r\n\r\n    CASE\r\n        WHEN hl.value REGEXP 'Accepted (password|publickey)' THEN 'Information'\r\n        WHEN hl.value REGEXP 'Failed password|authentication failure' THEN 'Warning'\r\n        WHEN hl.value REGEXP 'session closed for user' THEN 'Information'\r\n        ELSE 'Other'\r\n    END AS Severidade,\r\n\r\n    h.host AS Host,\r\n    i.name AS Item,\r\n\r\n    hl.value AS Log_Completo\r\n\r\nFROM history_log hl\r\n\r\nINNER JOIN items i\r\n    ON i.itemid = hl.itemid\r\n\r\nINNER JOIN hosts h\r\n    ON h.hostid = i.hostid\r\n\r\nWHERE\r\n    h.host = '${Host}'\r\n    AND hl.value REGEXP 'sshd'\r\n    AND i.status = 0\r\n    AND h.status = 0\r\n\r\nORDER BY hl.clock DESC\r\nLIMIT 500;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
